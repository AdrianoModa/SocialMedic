<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tela_medicos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.androidc22.socialmedic.TelaMedicos">

    <RatingBar
        android:id="@+id/ratingBar18"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/btnMedico03"
        android:layout_below="@+id/btnMedico01"
        android:layout_toEndOf="@+id/btnMedico03"
        android:layout_toRightOf="@+id/btnMedico03"
        android:numStars="1" />

    <SearchView
        android:id="@+id/idBuscarMedicos"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/ratingBar1"
        android:layout_alignEnd="@+id/ratingBar1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/idBuscarMedicos"
        android:layout_marginEnd="47dp"
        android:layout_marginRight="47dp"
        android:layout_marginTop="24dp"
        android:layout_toLeftOf="@+id/ratingBar2"
        android:layout_toStartOf="@+id/ratingBar2"
        android:text="@string/m_dicos"
        android:textSize="20sp"
        tools:text="Medicos"
        tools:ignore="UnknownIdInLayout" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="48dp"
        android:layout_height="match_parent"
        android:layout_below="@+id/textView"
        android:layout_marginTop="26dp">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="400dp"
        android:layout_height="500dp"
        android:layout_alignEnd="@+id/ratingBar18"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/ratingBar18"
        android:layout_marginBottom="10dp">

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/relativeLayout"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView5"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView7"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView3"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        android:contentDescription=""
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView2"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView1"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        tools:ignore="ContentDescription" />

    <Button
        android:id="@+id/btnMedico06"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Ciclana Lima Vieira \nNutricionista"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="10sp"
        android:layout_below="@+id/imageView5"
        android:layout_toRightOf="@+id/relativeLayout"
        android:layout_toEndOf="@+id/relativeLayout"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/btnMedico05"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnMedico04"
        android:layout_toEndOf="@+id/relativeLayout"
        android:layout_toRightOf="@+id/relativeLayout"
        android:text="Ultrano Moreira de Souza \nClinico Geral"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="10sp"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/btnMedico04"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView3"
        android:layout_toEndOf="@+id/relativeLayout"
        android:layout_toRightOf="@+id/relativeLayout"
        android:text="Fulano Souza Moura \nPsicologo"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="10sp"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/btnMedico03"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_toEndOf="@+id/relativeLayout"
        android:layout_toRightOf="@+id/relativeLayout"
        android:text="Ciclano Alves Ferreira \nPediatra"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="10sp"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/btnMedico02"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Beltrano da Silva \nOrtopedista"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="10sp"
        android:layout_below="@+id/imageView1"
        android:layout_toRightOf="@+id/relativeLayout"
        android:layout_toEndOf="@+id/relativeLayout"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/btnMedico01"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView1"
        android:layout_toEndOf="@+id/relativeLayout"
        android:layout_toLeftOf="@+id/ratingBar2"
        android:layout_toRightOf="@+id/relativeLayout"
        android:text="Fulano Silveira Mendes \nClinico Geral"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textSize="10sp"
        tools:ignore="HardcodedText,RtlHardcoded,SmallSp,UnknownIdInLayout" />

    <RatingBar
        android:id="@+id/ratingBar3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:numStars="1"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/btnMedico03"
        android:layout_toEndOf="@+id/btnMedico03" />

    <RatingBar
        android:id="@+id/ratingBar6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:numStars="1"
        android:layout_below="@+id/btnMedico05"
        android:layout_toRightOf="@+id/btnMedico06"
        android:layout_toEndOf="@+id/btnMedico06" />

    <RatingBar
        android:id="@+id/ratingBar7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnMedico04"
        android:layout_toEndOf="@+id/btnMedico06"
        android:layout_toRightOf="@+id/btnMedico06"
        android:numStars="1" />

    <RatingBar
        android:id="@+id/ratingBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:numStars="1"
        android:layout_below="@+id/ratingBar3"
        android:layout_alignRight="@+id/ratingBar2"
        android:layout_alignEnd="@+id/ratingBar2"
        tools:ignore="UnknownIdInLayout" />

    <RatingBar
        android:id="@+id/ratingBar4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:numStars="1"
        android:layout_above="@+id/btnMedico05"
        android:layout_toRightOf="@+id/btnMedico05"
        android:layout_toEndOf="@+id/btnMedico05" />

    <RatingBar
        android:id="@+id/ratingBar1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/btnMedico02"
        android:layout_toRightOf="@+id/btnMedico01"
        android:numStars="1"
        android:layout_alignRight="@+id/ratingBar7"
        android:layout_alignEnd="@+id/ratingBar7"
        tools:ignore="RtlHardcoded" />


</RelativeLayout>
